{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-unused-vars":"off",
        "no-empty":"off",
        "no-console":"off",
        "no-redeclare":"off",
        "valid-typeof":"off"
    }
}